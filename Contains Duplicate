public class Solution {
    public boolean containsDuplicate(int[] nums) {
        Set<Integer> numset = new HashSet<Integer> (nums.length);
        
        for(int i = 0; i < nums.length; i++){
            if(numset.contains(nums[i])) return true;
            else numset.add(nums[i]);
        }
        return false;
        
    }
}
