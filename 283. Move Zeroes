public class Solution {
    public void moveZeroes(int[] nums) {
        int count = 0;
        for(int i = 0; i < nums.length; i++){
            if(nums[i] != 0)
               nums[count++] = nums[i];
            }
        
        for(int i = count; i < nums.length; i++)
               nums[i] = 0;
    }
}


class Solution {
    public void moveZeroes(int[] nums) {
        int zeroP = 0;
        for(int i = 0; i < nums.length; i++){
            if(nums[i] != 0)
                nums[zeroP++] = nums[i];
        }
        for(int i = zeroP; i < nums.length; i++)
            nums[i] = 0;
    }
}

class Solution {
    public void moveZeroes(int[] nums) {
        for(int i = 0, zero = 0; i < nums.length; i++){
            if(nums[i] != 0){
                int temp = nums[zero];
                nums[zero++] = nums[i];
                nums[i] = temp;
            }
        }
    }
}
