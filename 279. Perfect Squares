class Solution {
    public int numSquares(int n) {
	    int[] count = new int[n+1];
		Arrays.fill(count, Integer.MAX_VALUE);
		count[0] = 0;
		for(int i = 1; i < n + 1; i++){
		     int min = Integer.MAX_VALUE;
		     for(int j = 1; j * j <= i ; j++){
			     min = Math.min(min, 1 + count[i - j*j]);
			 }
			 count[i] = min;	
		}
        return count[n];
    }
}


class Solution {
    public int numSquares(int n) {
        int[] count = new int[n + 1];
        Arrays.fill(count, Integer.MAX_VALUE);
        count[0] = 0;
        for(int i = 1; i <= n; i++){
            int min = Integer.MAX_VALUE;
            for(int j = 1; j * j <= i; j++){
                count[i] = Math.min(count[i], count[i - j * j] + 1);
            }
        }
        return count[n];
    }
}
