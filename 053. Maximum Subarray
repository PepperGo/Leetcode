public class Solution {
    public int maxSubArray(int[] nums) {
        int[] maxnums = new int[nums.length];
        int max = nums[0];
        maxnums[0] = nums[0];
        for(int i = 1; i < nums.length; i++){
            maxnums[i] = nums[i] + (maxnums[i-1] > 0 ? maxnums[i-1] : 0);
            max = Math.max(max,maxnums[i]);
        }
        return max;
        
    }
}
