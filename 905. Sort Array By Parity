class Solution {
    public int[] sortArrayByParity(int[] A) {
        int odd = A.length - 1;
        for(int i = 0; i < odd; i++){
            while(A[i] % 2 == 1 && i < odd){
                int temp = A[i];
                A[i] = A[odd];
                A[odd] = temp;
                odd--;
            }
        }
        return A;
    }
}
