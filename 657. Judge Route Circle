class Solution {
    public boolean judgeCircle(String moves) {
        int x = 0, y = 0;
        for(int i = 0; i < moves.length(); i++){
            if(moves.charAt(i) == 'R') x++;
            else if(moves.charAt(i) == 'L') x--;
            else if(moves.charAt(i) == 'U') y++;
            else if(moves.charAt(i) == 'D') y--;
        }
        return x == 0 && y == 0;
    }
}


class Solution {
    public boolean judgeCircle(String moves) {
        int x = 0, y = 0;
        for(int i = 0; i < moves.length(); i++){
            char ch = moves.charAt(i);
            if(ch == 'R') x++;
            if(ch == 'L') x--;
            if(ch == 'U') y++;
            if(ch == 'D') y--;
        }
        return x == 0 && y == 0;
    }
}
