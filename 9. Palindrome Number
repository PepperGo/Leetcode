public class Solution {
    public boolean isPalindrome(int x) {
        if(x < 0) return false;
        long res = 0;
        long iniX = (long) x;
       
        while(x != 0){
            res = res*10 + x % 10;
            if(res < Integer.MIN_VALUE || res > Integer.MAX_VALUE) return false;
            x = x/10;
        }
        if(res == iniX)
           return true;
        else 
           return false;
    }
}
